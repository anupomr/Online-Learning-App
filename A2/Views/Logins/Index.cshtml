@model IEnumerable<A2.Models.Login>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoginNameNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LoginNameNavigation.StudentId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LoginName">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LoginName">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LoginName">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
